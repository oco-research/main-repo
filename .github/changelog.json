{
  "combine": {
    "oco-test/buddy-repo:main": {}
  },
  "loader": {
    "name": "PullRequest",
    "config": {
      "exclude": {
        "labels": ["release"]
      },
      "groupBy": {
        "name" : "labels",
        "config": {
          "Enhancement": ["enhancement"],
          "Bugs": ["bug"]
        }
      }
    }
  },
  "output": {
    "override": false,
    "template": "pullrequest-issue",
    "filename": "CHANGELOG.md"
  }
}
